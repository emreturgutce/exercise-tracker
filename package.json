{
  "name": "exercise-tracker",
  "version": "1.0.0",
  "description": "Basic Typescript-Express Boilerplate",
  "main": "app.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .dist/index.js",
    "dev": "env-cmd -e development nodemon ./src/index.ts",
    "test": "env-cmd -e test jest --watch --runInBand --detectOpenHandles --logHeapUsage",
    "coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
