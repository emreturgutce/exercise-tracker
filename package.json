    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "sequelize": "^6.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.7",
    "@types/morgan": "^1.9.1",
    "@types/mysql": "^2.15.15",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^14.0.26",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-jest": "^26.1.0",
    "eslint": "^7.5.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
